     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/jean/ontologies/coralreefs">
        <rdfs:comment>Ontology on the land based coral reef pollution.</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/jean/ontologies/coralreefs#HasConsequence -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/jean/ontologies/coralreefs#HasConsequence">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/jean/ontologies/coralreefs#IsConsequenceOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/jean/ontologies/coralreefs#LandBasedType"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/jean/ontologies/coralreefs#PollutionType"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/jean/ontologies/coralreefs#HasPriorityLevel -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/jean/ontologies/coralreefs#HasPriorityLevel">
