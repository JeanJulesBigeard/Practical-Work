    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/jean/ontologies/coralreefs#HasSolution -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/jean/ontologies/coralreefs#HasSolution">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/jean/ontologies/coralreefs#IsSolutionOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/jean/ontologies/coralreefs#LandBasedType"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/jean/ontologies/coralreefs#ActionToTake"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/jean/ontologies/coralreefs#IsConsequenceOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/jean/ontologies/coralreefs#IsConsequenceOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/jean/ontologies/coralreefs#PollutionType"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/jean/ontologies/coralreefs#LandBasedType"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/jean/ontologies/coralreefs#IsSolutionOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/jean/ontologies/coralreefs#IsSolutionOf">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/jean/ontologies/coralreefs#ActionToTake"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/jean/ontologies/coralreefs#LandBasedType"/>
    </owl:ObjectProperty>
    


