    <!-- http://pollution#Pathogens -->

    <owl:Class rdf:about="http://pollution#Pathogens">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/jean/ontologies/coralreefs#PollutionType"/>
        <rdfs:comment>Pathogen pollution.</rdfs:comment>
    </owl:Class>
    


    <!-- http://pollution#Sedimentation -->

    <owl:Class rdf:about="http://pollution#Sedimentation">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/jean/ontologies/coralreefs#PollutionType"/>
        <rdfs:comment>Sedimentation pollution.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/jean/ontologies/coralreefs#ActionToTake -->

    <owl:Class rdf:about="http://www.semanticweb.org/jean/ontologies/coralreefs#ActionToTake">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/jean/ontologies/coralreefs#IsSolutionOf"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/jean/ontologies/coralreefs#LandBasedType"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Actions to take in order to addres the problem.</rdfs:comment>
    </owl:Class>
    


